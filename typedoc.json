{
  "name": "ogcapi-js",
  "mode": "modules",
  "out": "doc",
  "theme": "default",
  "ignoreCompilerErrors": false,
  "preserveConstEnums": true,
  "external-modulemap": ".*packages\/([^\/]+)\/.*",
  "stripInternal": "false",
  "excludeNotExported": true,
  "excludePrivate": true,
  "exclude": [
    "packages/**/test/**/*.ts"
  ]
}